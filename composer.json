{
    "name": "pugofka/laravel-smsru-notification-channel",
    "description": "SmsRu Notifications Driver",
    "homepage": "https://github.com/pugofka/laravel-smsru-notification-channel",
    "license": "MIT",
    "require": {
        "php": ">=8.0",
        "illuminate/notifications": "9.*",
        "illuminate/support": "9.*",
        "zelenin/smsru": "~5"
    },
    "require-dev": {
        "mockery/mockery": "^0.9.5",
        "phpunit/phpunit": "9.*"
    },
    "autoload": {
        "psr-4": {
            "NotificationChannels\\SmsRu\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "NotificationChannels\\SmsRu\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "config": {
        "sort-packages": true
    }
}
